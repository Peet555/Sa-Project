<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controller.homePageController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="106.0" prefWidth="667.0">
         <children>
            <Pane prefHeight="100.0" prefWidth="306.0" HBox.hgrow="ALWAYS" />
            <ImageView fitHeight="153.0" fitWidth="119.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fitHeight="150.0" fitWidth="116.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="profileButton" mnemonicParsing="false" text="Profile">
               <HBox.margin>
                  <Insets right="40.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cartButton" mnemonicParsing="false" text="Shopping Cart">
               <HBox.margin>
                  <Insets right="30.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="orderHistoryButton" mnemonicParsing="false" text="Order History">
               <HBox.margin>
                  <Insets right="20.0" top="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="56.0" prefWidth="667.0" style="-fx-background-color: #a3e9a4;">
         <children>
            <ButtonBar prefHeight="56.0" prefWidth="105.0">
               <buttons>
                  <Button fx:id="homeButton" mnemonicParsing="false" style="-fx-background-radius: 50;" text="Home" />
               </buttons>
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </ButtonBar>
         </children>
      </HBox>
      <ScrollPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <content>
            <GridPane fx:id="gridPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
               <padding>
                  <Insets left="125.0" />
               </padding>
            </GridPane>
         </content>
      </ScrollPane>
   </children>
</VBox>
