            <ImageView fitHeight="150.0" fitWidth="116.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="profileButton" mnemonicParsing="false" style="-fx-background-color: #0d5e25;" text="Profile" textFill="WHITE">
               <HBox.margin>
                  <Insets right="40.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cartButton" mnemonicParsing="false" style="-fx-background-color: #0d5e25;" text="Shopping Cart" textFill="WHITE">
               <HBox.margin>
                  <Insets right="30.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="orderHistoryButton" mnemonicParsing="false" style="-fx-background-color: #0d5e25;" text="Order History" textFill="WHITE">
               <HBox.margin>
                  <Insets right="20.0" top="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="56.0" prefWidth="667.0" style="-fx-background-color: #a3e9a4; -fx-border-color: #056f00;">
         <children>
            <ButtonBar prefHeight="56.0" prefWidth="105.0">
               <buttons>
                  <Button fx:id="homeButton" mnemonicParsing="false" style="-fx-background-radius: 50;" text="Home">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font></Button>
               </buttons>
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </ButtonBar>
         </children>
      </HBox>
      <ScrollPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <content>
            <GridPane fx:id="gridPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
               <padding>
                  <Insets left="125.0" />
               </padding>
            </GridPane>
         </content>
      </ScrollPane>
   </children>
</VBox>
